<h1>Listing designs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Times rated</th>
    <th>Rating</th>
    <th>Times downloaded</th>
    <th>User</th>
  </tr>

<% @designs.each do |design| %>
  <tr>
    <td><%=h design.name %></td>
    <td><%=h design.description %></td>
    <td><%=h design.times_rated %></td>
    <td><%=h design.rating %></td>
    <td><%=h design.times_downloaded %></td>
    <td><%=h design.user_id %></td>
    <td><%= link_to 'Show', design %></td>
    <td><%= link_to 'Edit', edit_design_path(design) %></td>
    <td><%= link_to 'Destroy', design, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New design', new_design_path %>